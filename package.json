{
  "name": "pinia-plugin-persistedstate",
  "version": "1.0.1",
  "description": "Configurable persistence and rehydration of Pinia stores.",
  "repository": "https://github.com/prazdevs/pinia-plugin-persistedstate.git",
  "keywords": [
    "pinia-plugin"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "prepublishOnly": "npm run build",
    "release": "bumpp -c ':bookmark: release v%s' -t -p",
    "lint": "eslint --fix --ext .ts",
    "test:2": "vue-demi-switch 2 vue2 && jest",
    "test:3": "vue-demi-switch 3 && jest",
    "test": "pnpm run test:2 && pnpm run test:3"
  },
  "author": "Sacha 'PraZ' Bouillez",
  "license": "MIT",
  "devDependencies": {
    "@prazdevs/eslint-config-typescript": "^2.2.0",
    "@types/jest": "^27.0.2",
    "@vue/composition-api": "^1.3.3",
    "bumpp": "^7.1.1",
    "eslint": "7",
    "jest": "^27.3.1",
    "pinia": "^2.0.0-rc.14",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "tsup": "^5.5.0",
    "typescript": "^4.4.4",
    "vue": "^3.2.21",
    "vue-demi": "^0.12.0",
    "vue2": "npm:vue@2"
  },
  "peerDependencies": {
    "pinia": "^2.0.0"
  },
  "dependencies": {
    "shvl": "^2.0.3"
  }
}
